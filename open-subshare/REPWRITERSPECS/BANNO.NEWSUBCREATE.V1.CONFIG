 IF TOCFILECREATED=TRUE THEN
  POPUPMESSAGE(0,"Required TOC Letterfiles "+CTRLCHR(34)+"BANNO.NEWSUBACCT.TERMS.xx"+CTRLCHR(34)+
                 " have been created, please update them as needed.")
