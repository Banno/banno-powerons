END [DEFINE]

SETUP
[* Set default values
*]
 DEBUGMODE=DEBUGMODEOFF

 IF DEBUGMODE=DEBUGMODEON THEN
  DATAFILETYPE="LETTER"
 ELSE
  DATAFILETYPE="DATA"

 CALL INIT
  VALIDINPUT=FALSE
  WHILE VALIDINPUT=FALSE
   DO
    CALL DISPLAYHTML
    CALL PARSEHTML
    CALL VALIDATEINPUT
   END
 CALL WRITEFILE
 CALL CREATETOCLETTERFILES
END [SETUP]

PRINT TITLE="BANNO.NEWSUBCREATE.V1.CONFIG"
 SUPPRESSNEWLINE
END [PRINT]

